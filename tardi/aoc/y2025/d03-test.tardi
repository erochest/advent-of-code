uses: ../io
uses: ../testing
uses: std/io
uses: std/strings
uses: std/vectors
uses: ./d03

"select-first should return the highest number and position"
{ 1 2 3 4 5 3 2 1 } select-first 2vector
{ 5 4 } assert==

"select-first should not return the final number as highest"
{ 1 2 3 4 5 3 2 1 9 } select-first 2vector
{ 5 4 } assert==

"select-first should return the first highest digit"
{ 2 3 4 6 3 8 3 3 2 8 4 5 2 3 4 1 3 } select-first 2vector
{ 8 5 } assert==

"select-second should return the highest number"
{ 3 2 1 9 } select-second
9 assert==

"select-digits should return the highest digits: 5 3"
{ 1 2 3 4 5 3 2 1 } select-digits 2vector
{ 5 3 } assert==

"select-digits should return the highest digits: 5 9"
{ 1 2 3 4 5 3 2 1 9 } select-digits 2vector
{ 5 9 } assert==

"select-digits should return the highest digits: 8 9"
{ 1 2 3 4 5 3 2 1 8 9 } select-digits 2vector
{ 8 9 } assert==

"select-digits should return the highest digits: 8 8"
{ 2 3 4 6 3 8 3 3 2 8 4 5 2 3 4 1 3 } select-digits 2vector
{ 8 8 } assert==

"find-max-joltage should return the highest digits as a single number: 53"
